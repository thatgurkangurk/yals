{
  "name": "sveltekit-test",
  "version": "0.0.1",
  "devDependencies": {
    "@lucia-auth/adapter-drizzle": "^1.0.7",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^3.0.0",
    "@t3-oss/env-core": "^0.10.0",
    "@types/bun": "^1.1.0",
    "autoprefixer": "^10.4.19",
    "better-sqlite3": "^9.5.0",
    "drizzle-kit": "^0.20.17",
    "drizzle-orm": "^0.30.9",
    "lucia": "^3.1.1",
    "nanoid": "^5.0.7",
    "ora": "^8.0.1",
    "postcss": "^8.4.38",
    "svelte": "^4.2.7",
    "svelte-adapter-bun": "^0.5.2",
    "svelte-check": "^3.6.0",
    "tailwindcss": "^3.4.3",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.3",
    "zod": "^3.23.3"
  },
  "private": true,
  "scripts": {
    "db:migrate:create": "drizzle-kit generate:sqlite --config drizzle.config.ts",
    "db:migrate": "bun run scripts/migrate.ts",
    "dev": "bun run --bun vite dev",
    "build": "bun run --bun vite build",
    "preview": "bun run --bun vite preview",
    "check": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "bun run --bun svelte-kit sync && bun run --bun svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "trustedDependencies": [
    "@sveltejs/kit",
    "better-sqlite3",
    "es5-ext",
    "esbuild",
    "svelte-preprocess"
  ],
  "type": "module"
}
